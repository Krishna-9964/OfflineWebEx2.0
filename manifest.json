{
  "manifest_version": 3,
  "name": "OfflineWebEx",
  "version": "1.0",
  "permissions": [
    "scripting",
    "downloads",
    "activeTab",
    "history",
    "browsingData"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}